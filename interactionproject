<html>

<head>
    <title>p5js Template</title>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.5.3/p5.js"></script>
    
    <script>
    
        
        function setup() {
            createCanvas(800,600);
          
        }

        function draw() {
            triangle(mouseX+70,mouseY,mouseX+25,mouseY-25,mouseX+25,mouseY+25);// right 
            triangle(mouseX-70,mouseY,mouseX-25,mouseY+25,mouseX-25,mouseY-25);// left
            triangle(mouseX,mouseY+70,mouseX+25,mouseY+25,mouseX-25,mouseY+25);// down
            triangle(mouseX,mouseY-70,mouseX-25,mouseY-25,mouseX+25,mouseY-25);// up
            rectMode(CENTER);
            rect(mouseX,mouseY,50,50);
        }

         function mouseDragged(){
            fill(random(254),random(254),random(254));
            ellipse(mouseX,mouseY,105,105);
        }
        
        function mousePressed(){
            // outline
            noFill();
            stroke(random(255),random(255),0);
            strokeWeight(random(2,5));// between 2-5
        }
        
        function keyPressed(){
            // clear's everything
            background(random(2,255));
            noFill();
            stroke(255);
        }
    </script>
</head>

<body>
    <div id="code" class="container-fluid">
        <script src="https://gist-it.appspot.com/github/jessicac3732/p5js/blob/gh-pages/interaction-project.html?footer=0"></script>
    </div>


    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
    <script>
        $(window).bind("load", function() {
            $('#code').before($('#defaultCanvas0').remove());
        });
    </script>
</body>


</html>
